{
  "uid": "postgresql-metrics",
  "title": "PostgreSQL Metrics",
  "tags": ["postgresql", "zabbix", "patroni", "database"],
  "timezone": "browser",
  "editable": true,
  "refresh": "1m",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "host",
        "type": "query",
        "datasource": {
          "type": "alexanderzobnin-zabbix-datasource",
          "uid": "PA67C5EADE9207728"
        },
        "query": {
          "queryType": "host",
          "group": "Patroni Cluster",
          "host": "/.*/"
        },
        "multi": true,
        "includeAll": true,
        "current": {
          "text": "All",
          "value": "$__all"
        },
        "refresh": 1
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "PostgreSQL: Connections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": {
        "type": "alexanderzobnin-zabbix-datasource",
        "uid": "PA67C5EADE9207728"
      },
      "targets": [
        {
          "queryType": "0",
          "group": { "filter": "Patroni Cluster" },
          "host": { "filter": "$host" },
          "application": { "filter": "" },
          "itemTag": { "filter": "" },
          "item": { "filter": "/PostgreSQL: Connections (sum|active|idle|waiting)/" },
          "functions": [],
          "options": {
            "showDisabledItems": false
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "custom": {
            "lineWidth": 1,
            "fillOpacity": 10,
            "spanNulls": true
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
